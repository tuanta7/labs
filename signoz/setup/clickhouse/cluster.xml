<?xml version="1.0"?>
<clickhouse>
    <zookeeper>
    <node index="1">
        <host>zookeeper</host>
        <port>2181</port>
    </node>
    </zookeeper>
    <remote_servers>
        <cluster>
            <shard>
                <replica>
                    <host>clickhouse</host>
                    <port>9000</port>
                </replica>
            </shard>
        </cluster>
    </remote_servers>
</clickhouse>
